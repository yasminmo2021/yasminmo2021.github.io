<script>
(function () {
	let categoryLabel = '';
	let site = 'unknown';

	if (window?.forbes?.['preferences']) {
		site = 'preferences';
	} else if (window?.forbes?.['simple-site']) {
		site = 'simple-site';
	} else if (window?.forbes?.['digital-assets']) {
		site = 'digital-assets';
	}

	const { href: url } = window.location;
	const isProfilesPage = ['/profile/claim/find', '/profile/claim/?naturalId='].some((path) => url.includes(path));
	const isFDAPage = url.includes('/digital-assets/');
	const isSignIn = url.includes('/sign-in');
	// For sites that dont have window.forbes available, default to prod for now
	const isProd = window.forbes?.[site]?.isProd ?? true;
	const queryParamRegCode = window.location.search.match(
			new RegExp('[?&]' + 'regcode' + '=([^&#]*)'),
		)?.[1];
	const emailRegExp = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
	let storedRegCode = queryParamRegCode;

	const registerFormFieldsAndButtons = {
		detailsFields: ['fname', 'lname', 'joblevel', 'jobfunction'],
		announcementsAndConsentCheckboxes: ['announcements', 'consent'],
		termsAndPolicyButtons: ['terms-of-service', 'privacy-policy'],
		emailAndPasswordFields: ['email-address', 'password-field'],
		confirmPasswordFields: ['email-address', 'password-field', 'confirm-password-field'],
		socialNetworkButtons: ['google-auth', 'fb-auth', 'linkedin-auth'],
	};

	const loginActions = {
		REGISTER: 'Register',
		SIGN_IN: 'Sign In'
	}

	const loginState = {
		screens: {
			ENTER_EMAIL: 'email',
			ENTER_REG_CODE: 'reg-code',
			ENTER_PASSWORD: 'password',
			FORGOT_PASSWORD: 'forgot-password',
			CONFIRM_PASSWORD: 'confirm-password',
			FINISH_ACCOUNT: 'finish-account',
		},
		boundListeners: [],
		boundElements: [],
	};

	function pushLoginEvent(eventLabel) {
		window.dataLayer.push({
			event: 'analyticsEvent',
			'event category': 'Account',
			'event action': 'Account modal',
			'event label': eventLabel,
		});
	}

	function pushRegisterEvent() {
		const eventLabel = site === 'preferences' ? 'pref center' : window.registerSource || 'General';

		window.dataLayer.push({
			event: 'analyticsEvent',
			'event category': 'Zephr',
			'event action': 'Register',
			'event label': eventLabel,
		});
	}

	function pushSignInEvent() {
		const eventLabel = site === 'preferences' ? 'pref center' : window.registerSource || 'General';

		window.dataLayer.push({
            event: 'analyticsEvent',
            'event category': 'Zephr',
            'event action': 'Sign In',
            'event label': eventLabel,
        });
	}

	const setPathToClaimFind = () => {
		const hasListProfile = window.Zephr?.utils?.shouldShowListProfile() || false;
		const params = (new URL(window.location.href)).searchParams;
		const hasCustomRedirectParam = params.get('customRedirect') === 'true';

		if (hasListProfile && hasCustomRedirectParam) {
			window.history.pushState({}, '', '/profile/claim/find');
		}
	};

	async function executeCallbackQueue(callbackQueue = []) {
		await Promise.all(callbackQueue.map(async ({ callback }) => {
			try {
				await callback();
			} catch (error) {
				console.error(`Error executing callback: ${callback.name || 'unnamed function'}!`, error);
			}
		}));
	}

	const addUserToB2BAccount = async () => {
		if (!storedRegCode) {
			return true;
		}

		try {
			const response = await fetch(
				`https://${
					isProd ? '' : 'staging-'
				}account.forbes.com/api-data/handle-account-regcode`,
				{
					method: 'PUT',
					body: JSON.stringify({
						registrationCode: storedRegCode,
					}),
					headers: {
						Accept: 'application/json',
						'Content-type': 'application/json',
					},
				},
			);

			const results = await response.json();

			return results.error ? false : true;
		} catch (error) {
			console.error('Error handling account registration code::', error);
			return false;
		}
	};

	function buildErrorMessage(errorText) {
		const zephrErrorMessageDivs = document.querySelectorAll(
			'.zephr-registration-form-response-message-error',
		);

		if (zephrErrorMessageDivs.length > 0) {
			return;
		}

		const registrationFormSubmitButton = document.querySelector(
			'.zephr-registration-form-button[type="submit"]',
		);
		const errorDiv = document.createElement('div');
		const errorCopy = document.createTextNode(errorText);
		errorDiv.classList =
			'zephr-registration-form-response-message zephr-registration-form-response-message-error';
		errorDiv.append(errorCopy);
		registrationFormSubmitButton.before(errorDiv);
	}

	function showAccountConfirmation() {
		const urlParams = new URLSearchParams(window.location.search);

		const confirmationHeader = '' || 'Success!';
		const confirmationCtaCopy = '' || 'Continue Reading';
		const confirmationCtaURL = '' || urlParams.get('redirect') || `https://www${isProd ? '.' : '-staging.'}forbes.com`;

		const container = document.createElement('div');

		container.innerHTML = `
			<div class="account-confirmation">
				<div class="account-confirmation-header">
					<h2>${confirmationHeader}</h2>
				</div>
				<div class="account-confirmation-ctas">
					<button onclick="window.location.href='${confirmationCtaURL}';">${confirmationCtaCopy}</button>
				</div>
			</div>`;

		const regFormRoot = document.querySelector(
			'[id^="zephr-registration-form-"]',
		);

		regFormRoot.innerHTML = container.innerHTML;
	}

	const cookieIsSetLogin = async () => {
        if(document.cookie.split(';').find((cookie) => (cookie.indexOf(`fbs-zephr-access=`) > -1))) {
			pushSignInEvent();
			setPathToClaimFind();

			const passesRegCodeCheck = await addUserToB2BAccount();

			if (!passesRegCodeCheck) {
				buildErrorMessage('Please enter a valid registration code.');
				return;
			} else if (storedRegCode) {
				showAccountConfirmation();
				return;
			}

			await executeCallbackQueue(window.loginCallbackQueue);

            window.location.reload();
        } else {
            setTimeout(async () => await cookieIsSetLogin(), 500);
        }
    }

	const cookieIsSetRegistration = async () => {
		if (
			document.cookie
				.split(';')
				.find((cookie) => cookie.indexOf(`fbs-zephr-access=`) > -1)
		) {
			pushRegisterEvent();

			if (isFDAPage) {
				await registerFdaUser();
			}

			if (storedRegCode) {
				showAccountConfirmation();
				return;
			}

			await executeCallbackQueue(window.registrationCallbackQueue);

			window.location.reload();
		} else {
			setTimeout(async () => await cookieIsSetRegistration(), 500);
		}
	};

	// When login harness is loaded in pref center /sign-in, send event immediately
	if(site === 'preferences' && isSignIn){
		pushLoginEvent('shown');
	}

	function getCookieValue(cookies = '', cookieName) {
		return (cookies.split(';').find((cookie) => cookie.indexOf(`${cookieName}=`) > -1) || '').split('=')[1] || '';
	}

	async function registerFdaUser() {
		const zephrSessionID = getCookieValue(document.cookie, 'blaize_session');
		const data = {
			zephrSessionID,
			scopes: [0],
			action: 0,
		};

		try {
			const response = await fetch(`https://fda${isProd ? '' : '-dev'}.forbes.com/v2/extendedprofile`, {
				method: 'POST',
				body: JSON.stringify(data),
				headers: {
					Accept: 'application/json',
					'Content-type': 'application/json',
				},
			});

			const results = await response.json();
			console.log('Registering FDA user::', results.message);
		} catch (error) {
			console.error('Error registering FDA user::', error);
		}
	}

	app.$$set({'onRegister': (function() {
		if (((window.Zephr || {}).utils || {}).callDataLayerUpdate) {
			window.Zephr.utils.callDataLayerUpdate().then(async function () {
				// open onboarding tab as quickly as possible to prevent browser pop-up blocking
				if(!isProfilesPage) {
					window.open(`https://${isProd ? '' : 'staging-'}account.forbes.com/onboarding`);
				}

				cookieIsSetRegistration();
			});
		} else {
			pushRegisterEvent();
		}
	})});

    app.$$set({'onLogin': (function() {
        if (((window.Zephr || {}).utils || {}).callDataLayerUpdate) {
            window.Zephr.utils.callDataLayerUpdate().then(function () {
                cookieIsSetLogin();
            });
        }
    })});

	function unbindListeners() {
		loginState.boundListeners.forEach((listener) => {
			listener.elem.removeEventListener(listener.event, listener.callback);
		});
		loginState.boundListeners = [];
	}

	function bindListener(elem, event, callback, useCapture = false) {
		loginState.boundListeners.push({
			elem,
			event,
			callback,
		});
		elem.addEventListener(event, callback, useCapture);
	}

	function removeElements() {
		loginState.boundElements.forEach((elem) => {
			elem.remove();
		});
		loginState.boundElements = [];
	}

	/**
	 * Adds an element to the DOM in the provided location.
	 * @param {HTMLElement} elem Element to insert
	 * @param {HTMLElement} refElem Element to use as the target reference
	 * @param {InsertPosition} location How to insert this element.
	 */
	function addElement(elem, refElem, location) {
		loginState.boundElements.push(elem);
		refElem.insertAdjacentElement(location, elem);
	}

	function generateUUID() {
		// Generate a random hexadecimal string
		function generateHex(length) {
			let result = '';
			const characters = 'abcdef0123456789';
			for (let i = 0; i < length; i++) {
				result += characters.charAt(Math.floor(Math.random() * characters.length));
			}
			return result;
		}

		return ( generateHex(3) + '-' + generateHex(12));
	}

	/**
	 * Adds the Google reCAPTCHA challenge to the Zephr form
	 * 1. Create div to attach the challenge
	 * 2. Attach Google reCAPTCHA api.js to the div
	 * 3. Execute challenge on script load
	 * 4. Hide and update zephr input storing challenge token 
	 */ 
	function recaptchaHandler() {
		const parentContainer = document.querySelector('.zephr-registration-form');
		const storedUUID = generateUUID();

		// Adding reCAPTCHA challenge to the form
		const recaptchaContainer = document.createElement('div');
		const recaptchaContainerId = `${storedUUID}-login-g-recaptcha`;
		recaptchaContainer.setAttribute('id', recaptchaContainerId);
		addElement(recaptchaContainer, parentContainer, 'beforeend');

		// Adding reCAPTCHA script to the form
		const recaptchaSitekey = isProd ? '6LeLC2wpAAAAAHxdEQ59hxseRouCtGrCo22PUjuu' : '6LeIxAcTAAAAAJcZVRqyHh71UMIEGNQ_MXjiZKhI';
		const recaptchaScript = document.createElement('script');
		recaptchaScript.src = 'https://www.google.com/recaptcha/api.js';

		// Called on reCAPTCHA script load. Renders challenge and handles form updates.
		function gRecaptchaExecute() {
			grecaptcha.ready(() => {
				let widgetId;
				const submitButton = document.querySelector('.zephr-registration-form-button[type="submit"]');

				bindListener(submitButton, 'click', () => {
					//add check for captcha input
					const recaptchaInput = document.querySelector('[name^=zephr-registration-form-recaptcha-response]');

					if (!recaptchaInput.value || recaptchaInput.value === 'default') {
						grecaptcha.reset(widgetId);
						// Reset takes a bit and have to wait before executing again
						setTimeout(() => {
							recaptchaInput.value = 'default';
							grecaptcha.execute(widgetId);
						}, 100);
					}
				});

				// Called after challenge success. Updates the hidden input on the zephr form
				window[`grecaptchaexecute-${storedUUID}`] = (token) => {
					const recaptchaInput = document.querySelector('[name^=zephr-registration-form-recaptcha-response]');
					recaptchaInput.value = token;
					recaptchaInput.dispatchEvent(new Event('input', {
						view: window,
						bubbles: true,
						cancelable: true,
					}));
				}

				// After between 45s and 2min the reCAPTCHA token expires. Handles 
				// showing the challenge again + refreshing the token 
				window[`grecaptchaexpired-${storedUUID}`] = () => {
					try {
						const recaptchaInput = document.querySelector('[name^=zephr-registration-form-recaptcha-response]');
						// if challenge never succeeded, we don't want to reset
						if (!recaptchaInput.value) {
							return
						}
						grecaptcha.reset(widgetId);
						// Reset takes a bit and have to wait before executing again
						setTimeout(() => {
							recaptchaInput.value = 'default';
							grecaptcha.execute(widgetId);
						}, 100);
						console.log('reCAPTCHA reset and executed successfully');
					} catch (error) {
						console.error('Error resetting and executing reCAPTCHA:', error);
					}
				};

				// Create challenge on the DOM. Attaches to the parent div created above.
				const renderRecaptcha = () => {
					return grecaptcha.render(recaptchaContainerId, {
						'sitekey': recaptchaSitekey,
						'size': 'invisible',
						'callback': `grecaptchaexecute-${storedUUID}`,
						'expired-callback': `grecaptchaexpired-${storedUUID}`
					});
				}

				// Shows the challenge to the user
				const executeRecaptcha = (widgetId) => {
					grecaptcha.execute(widgetId).then(
						() => {
							// Successful challenge updates user attribute value recaptcha-response
						},
						(error) => {
							// Handle reCAPTCHA verification failure
							console.error('reCAPTCHA verification failed:', { error });
						}
					);
				}

				// Modify reCAPTCHA challenge on the form once api is ready
				widgetId = renderRecaptcha();
				// Execute reCAPTCHA challenge on the form once api is ready
				executeRecaptcha(widgetId);
			});
		}

		recaptchaScript.onload = gRecaptchaExecute;
		addElement(recaptchaScript, parentContainer, 'beforeend');
	}

	const hasValidRegCodeScreenInputs = (
		registerFormEmailField,
		registerFromRegCodeField,
	) =>
		registerFromRegCodeField.value !== '' &&
		emailRegExp.test(registerFormEmailField.value);

	function bindRegCodeScreenListeners() {
		// register form - email field
		const registerFormEmailField = document.querySelector(
			'.zephr-registration-form-input',
		);
		bindListener(registerFormEmailField, 'click', () => {
			pushLoginEvent('clicks-email-address');
		});

		// register form - sign up/in or continue button
		const registrationFormSubmitButton = document.querySelector(
			'.zephr-registration-form-button[type="submit"]',
		);
		bindListener(registrationFormSubmitButton, 'click', () => {
			pushLoginEvent('clicks-signin-or-register');
		});

		const registerFromRegCodeField = document.querySelector(
			'[name^="zephr-registration-form-account-code-"]',
		);
		bindListener(registerFromRegCodeField, 'click', () => {
			pushLoginEvent('clicks-registration-code');
		});

		// Set the input field to what we have in our query param
		registerFromRegCodeField.value = queryParamRegCode;
		// Trigger input event so zephr's form picks up the change
		registerFromRegCodeField.dispatchEvent(new Event('input'));

		registerFormEmailField.addEventListener('input', () => {
			registrationFormSubmitButton.disabled = !hasValidRegCodeScreenInputs(
				registerFormEmailField,
				registerFromRegCodeField,
			);
		});

		registerFromRegCodeField.addEventListener('input', () => {
			storedRegCode = registerFromRegCodeField.value;
			registrationFormSubmitButton.disabled = !hasValidRegCodeScreenInputs(
				registerFormEmailField,
				registerFromRegCodeField,
			);
		});

		registrationFormSubmitButton.disabled = !hasValidRegCodeScreenInputs(
			registerFormEmailField,
			registerFromRegCodeField,
		);
	}

	function bindEmailScreenListeners() {
		// register form - email field
		const registerFormEmailField = document.querySelector('.zephr-registration-form-input');
		bindListener(registerFormEmailField, 'click', () => {
			pushLoginEvent('clicks-email-address');
		});

		// register form - continue with social networks
		const registrationFormSocialNetworkButtons = document.querySelectorAll('.zephr-registration-form-social-sign-in-button');
		registrationFormSocialNetworkButtons.forEach((socialNetworkButton, socialNetworkButtonIndex) => {
			const socialNetworkButtonName = registerFormFieldsAndButtons.socialNetworkButtons[socialNetworkButtonIndex] || '';

			bindListener(socialNetworkButton, 'click', () => {
				pushLoginEvent(`clicks-${socialNetworkButtonName}`);
			});
		});

		// add social sign in buttons to flex container to center them
		if (registrationFormSocialNetworkButtons.length > 0) {
			const socialSignInContainer = document.createElement('div');
			const parentLoginContainer = document.querySelector('.zephr-registration-form');
			socialSignInContainer.classList = 'social-sign-in-flex-container';

			registrationFormSocialNetworkButtons.forEach((ele) => {
				socialSignInContainer.appendChild(ele);
			});

			addElement(socialSignInContainer, parentLoginContainer, 'beforeend');
		}

		// register form - sign up/in or continue button
		const registrationFormSubmitButton = document.querySelector('.zephr-registration-form-button[type="submit"]');
		bindListener(registrationFormSubmitButton, 'click', () => {
			pushLoginEvent('clicks-signin-or-register')
		});

		// Email input validation
		const emailRegExp = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;

		registerFormEmailField.addEventListener('input', () => {
			registrationFormSubmitButton.disabled = !emailRegExp.test(registerFormEmailField.value);
		});
	}

	function bindPasswordScreenListeners() {
		// register form - email address, password input fields
		const emailAndPasswordFields = document.querySelectorAll('.zephr-registration-form-input');
		emailAndPasswordFields.forEach((field, fieldIndex) => {
			const fieldName = registerFormFieldsAndButtons.emailAndPasswordFields[fieldIndex] || '';

			bindListener(field, 'click', () => {
				pushLoginEvent(`clicks-${fieldName}`);
			});
		});
	}

	function bindConfirmPasswordScreenListeners() {
		// register form - confirm password input field
		const emailAndPasswordFields = document.querySelectorAll('.zephr-registration-form-input');
		emailAndPasswordFields.forEach((field, fieldIndex) => {
			const fieldName = registerFormFieldsAndButtons.confirmPasswordFields[fieldIndex];

			if(fieldName) {
				bindListener(field, 'click', () => {
					pushLoginEvent(`clicks-${fieldName}`);
				});
			}
		});

		// register form - confirm password continue button
		const continueButton = document.querySelector('.zephr-registration-form-button[type="submit"]');
		
		// useCapture set to true due to Zephr having built-in event cancelling
		bindListener(continueButton, 'click', () => {
			pushLoginEvent('clicks-continue');
		}, true);
	}

	function bindForgotPasswordScreenListeners() {
		// @todo
	}

	const handleDisplayNameUpdate = (displayName, firstNameInput, lastNameInput) => {
		const displayNameString = `${firstNameInput.value} ${lastNameInput.value.charAt(0)}`.trim();
		displayName.value = displayNameString;
		displayName.dispatchEvent(new Event('input', {
			view: window,
			bubbles: true,
			cancelable: true,
		}));
	}

	function bindCreateAccountScreenListeners() {
		// Adding recaptcha challenge and hidden input updates if input exists on the form
		const recaptchaInput = document.querySelector('[name^=zephr-registration-form-recaptcha-response]');
		if (recaptchaInput) {
			recaptchaHandler();
		}

		const registrationConsent = document.querySelector('label[for*="zephr-registration-form-affirmative-consent-account-creation"]');
		const registrationFieldCheckbox = registrationConsent?.querySelector('.zephr-registration-form-checkmark');
		const submitButton = document.querySelector('.zephr-registration-form-button[type="submit"]');
		const displayName = document.querySelector('[placeholder="Display Name"]');
		const firstNameInput = document.querySelector('[id*="zephr-registration-form-first-name"]');
		const lastNameInput = document.querySelector('[id*="zephr-registration-form-last-name"]');
		const personalDetailsFields = document.querySelectorAll('.zephr-registration-form-input');
		const announcementsAndConsentCheckboxes = document.querySelectorAll('.zephr-registration-form-checkbox');
		const registerFormtermsAndPolicyButtons = document.querySelectorAll('.zephr-registration-form-text p a');

		displayName.style.display = 'none';

		bindListener(firstNameInput, 'input', () => {
			handleDisplayNameUpdate(displayName, firstNameInput, lastNameInput);
		});

		bindListener(lastNameInput, 'input', () => {
			handleDisplayNameUpdate(displayName, firstNameInput, lastNameInput);
		});

		personalDetailsFields.forEach((field, fieldIndex) => {
			const detailsFieldName = registerFormFieldsAndButtons.detailsFields[fieldIndex] || '';

			bindListener(field, 'click', () => {
				pushLoginEvent(`clicks-${detailsFieldName}`);
			});
		});

		// register form - consent checkboxes
		announcementsAndConsentCheckboxes.forEach((checkbox, checkboxIndex) => {
			let pushedEvent = false;
			const checkboxName = registerFormFieldsAndButtons.announcementsAndConsentCheckboxes[checkboxIndex] || '';

			bindListener(checkbox, 'click', () => {
				if (!pushedEvent) {
					pushLoginEvent(`clicks-${checkboxName}`);
					pushedEvent = true;
				}
			});
		});

		// register form - links
		registerFormtermsAndPolicyButtons.forEach((button, buttonIndex) => {
			const buttonName = registerFormFieldsAndButtons.termsAndPolicyButtons[buttonIndex] || '';

			bindListener(button, 'click', () => {
				pushLoginEvent(`clicks-${buttonName}`);
			});
		});

		// register form - submit button
		bindListener(submitButton, 'click', () => {
			const submitBtnEventLabel = submitButton.textContent.trim().toLowerCase();
			pushLoginEvent(`clicks-${submitBtnEventLabel}`);
		});
	}

	function searchSpansFind(loginFormRoot, searchText) {
		const loginSpans = Array.from(loginFormRoot.getElementsByTagName('span'));

		const foundSpan = loginSpans.find((span) => {
			const spanText = span.innerText;
			return spanText.toUpperCase() === searchText;
		});

		return foundSpan;
	}

	function getCurrentScreen() {
		const loginFormRoot = document.querySelector('[id^="zephr-user-form-root-"]');

		const email = searchSpansFind(loginFormRoot, 'EMAIL');
		const password = searchSpansFind(loginFormRoot, 'PASSWORD');
		const twoFactorCode = searchSpansFind(loginFormRoot, 'CODE');
		const confirmPassword = searchSpansFind(loginFormRoot, 'CONFIRM PASSWORD');
		const firstName = searchSpansFind(loginFormRoot, 'FIRST NAME');
		const registerFromRegCodeField = document.querySelector(
			'[name^="zephr-registration-form-account-code-"]',
		);

		if (registerFromRegCodeField) {
			return loginState.screens.ENTER_REG_CODE;
		} else if (email && !password && !twoFactorCode) {
			return loginState.screens.ENTER_EMAIL;
		} else if (confirmPassword) {
			return loginState.screens.CONFIRM_PASSWORD;
		} else if (email && password) {
			return loginState.screens.ENTER_PASSWORD;
		} else if (twoFactorCode) {
			return loginState.screens.FORGOT_PASSWORD;
		} else if (firstName) {
			return loginState.screens.FINISH_ACCOUNT;
		} else {
			console.error('Unknown login screen!');
		}
	}

	function enterScreen(toScreen, fromScreen) {
		unbindListeners();
		removeElements();

		switch(toScreen) {
			case loginState.screens.ENTER_EMAIL:
				bindEmailScreenListeners();
				break;
			case loginState.screens.ENTER_PASSWORD:
				bindPasswordScreenListeners();
				break;
			case loginState.screens.ENTER_REG_CODE:
				bindRegCodeScreenListeners();
				break;
			case loginState.screens.FORGOT_PASSWORD:
				bindForgotPasswordScreenListeners();
				break;
			case loginState.screens.CONFIRM_PASSWORD:
				bindConfirmPasswordScreenListeners();
				break;
			case loginState.screens.FINISH_ACCOUNT:
				bindCreateAccountScreenListeners();
				break;
			default:
				console.error('Entering unknown screen', toScreen);
		}
	}

	function handleScreenState() {
		const currentScreen = getCurrentScreen();
		if (loginState.currentScreen !== currentScreen) {
			const oldScreen = loginState.currentScreen;
			loginState.currentScreen = currentScreen;
			enterScreen(currentScreen, oldScreen);
		}
	}

	function watchDom() {
		const observer = new MutationObserver((mutations) => {
			handleScreenState();
		});

		const loginFormRoot = document.querySelector('[id^="zephr-user-form-root-"]');

		observer.observe(loginFormRoot, {
			childList: true,
			subtree: true,
		});
	}

	// When sign-in modal is opened in simple site
	document.addEventListener('fbs-zephr-modal-opened', (event) => {
		if (event.detail.modalId === 'sign-in') {
			 	pushLoginEvent('shown');
		}
	});

	const handleRegCodeLink = () => {
		const renderRegCodeLink = document.querySelector('.zephr-form-link');

		if (renderRegCodeLink) {
			renderRegCodeLink.click()
		}
	}

	handleRegCodeLink();
	handleScreenState();
	watchDom();
})();
